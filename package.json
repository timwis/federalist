    "mocha": "2.3.4",
    "nodemon": "^1.3.7",
    "selenium-standalone": "^4.9.0",
    "sinon": "1.17.2",
    "watchify": "^3.2.2",
    "webdriverio": "^3.4.0"
  },
  "engines": {
    "node": ">=4.4.5 <5",
    "npm": ">=2.15.1 <3"
  }
}
